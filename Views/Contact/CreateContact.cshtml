@model MyPortfolio_MVC.Models.TblContact
@{
    ViewBag.Title = "CreateContact";
    Layout = "~/Views/AdminLayout/Layout.cshtml";
}

<h2>Yeni İletişim Bilgisi Ekle</h2>



<form method="post">
    <label>Telefon</label>
    @Html.TextBoxFor(x => x.Phone , new { @class = "form-control" })
    <br />
    <label>Mail</label>
    @Html.TextBoxFor(x => x.Email, new { @class = "form-control" })
    <br />


    <button class="btn btn-outline-primary">Kaydet</button>
    <br />

</form>


